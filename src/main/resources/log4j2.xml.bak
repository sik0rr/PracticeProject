<Configuration status='off'>
    <Properties>
        <Property name='logFilePath'>resources/log.log</Property>
    </Properties>
    <Appenders>
        <Console name='Console' target='SYSTEM_OUT'>
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
        </Console>
        <File name='processLog' fileName="logs/process.log">
            <PatternLayout>
                <Pattern>%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n</Pattern>
            </PatternLayout>
        </File>
		<File name='errorLog' fileName="logs/errors.log">
            <PatternLayout>
                <Pattern>%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n</Pattern>
            </PatternLayout>
        </File>
    </Appenders>
    <Loggers>
        <Root level='all'>
            <AppenderRef ref='processLog' level="info"/>
			<AppenderRef ref='errorLog' level="error"/>
            <AppenderRef ref='Console'/>
        </Root>
    </Loggers>
</Configuration>